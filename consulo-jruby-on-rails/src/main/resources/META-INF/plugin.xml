<consulo-plugin>
	<id>consulo.jruby.rails</id>
	<name>JRuby on Rails</name>
	<version>SNAPSHOT</version>

	<depends>consulo.ruby.rails</depends>

	<tags>
		<tag>language.extension</tag>
		<tag>web.framework</tag>
		<tag>experimental</tag>
	</tags>

	<experimental>true</experimental>

	<extensions defaultExtensionNs="com.intellij">
		<moduleExtensionProvider key="jruby-on-rails"
								 parentKey="jruby"
								 icon="/org/jetbrains/plugins/ruby/rails/rails_small.png"
								 name="Rails"
								 immutableClass="consulo.jruby.rails.module.extension.JRubyOnRailsModuleExtension"
								 mutableClass="consulo.jruby.rails.module.extension.JRubyOnRailsMutableModuleExtension" />
	</extensions>
</consulo-plugin>