<consulo-plugin url="https://github.com/consulo/incubating-consulo-ruby">
	<id>consulo.ruby.rails</id>
	<name>Ruby on Rails</name>
	<version>SNAPSHOT</version>

	<depends>consulo.ruby</depends>

	<tags>
		<tag>language.extension</tag>
	</tags>

	<experimental>true</experimental>

	<extensions defaultExtensionNs="com.intellij">
		<moduleExtensionProvider key="ruby-on-rails"
								 parentKey="ruby"
								 name="Rails"
								 icon="consulo.ruby.api.RubyApiIconGroup@rails.rails_small"
								 immutableClass="consulo.ruby.rails.module.extension.BaseRubyOnRailsModuleExtension"
								 mutableClass="consulo.ruby.rails.module.extension.BaseRubyOnRailsMutableModuleExtension" />
	</extensions>
</consulo-plugin>